<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

<path>engpanel_002.ac</path>

<animation>
      <type>noshadow</type>
	   <object-name>lowerpanel</object-name>
	   <object-name>EngineerTable</object-name>
	   <object-name>upperpanel</object-name>
</animation>  

<animation>
        <type>select</type>
      <object-name>disk</object-name>
      <object-name>poly</object-name>
      <object-name>rect</object-name>
        <condition>
	      <equals>
	        <property>sim/devel</property>
	        <value>true</value>
	      </equals>
        </condition>
</animation>  

<!-- Lower Panel Ilumination -->
   <animation>
   <object-name>lowerpanel</object-name>
   <object-name>EngineerTable</object-name>
   <type>material</type>
     <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
	 <red>0.5</red>
	 <green>0.48</green>
	 <blue>0.44</blue>
      </emission>
  </animation>
  

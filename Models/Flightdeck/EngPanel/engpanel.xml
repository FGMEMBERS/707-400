<!-- Define a objects set -->
   <animation>
		<name>Engpanelutil</name>
	   <object-name>lowerpanel</object-name>
	   <object-name>EngineerTable</object-name>
	   <object-name>upperpanel</object-name>
  </animation>

  
<!-- Cabin Ilumination -->
   <animation>
	   <object-name>Engpanelutil</object-name>
   <type>material</type>
     <emission> 
      <factor-prop>controls/lighting/cabin-lights</factor-prop>
		 <red>0.6</red>
		 <green>0.56</green>
		 <blue>0.54</blue>
      </emission>
  </animation>
  

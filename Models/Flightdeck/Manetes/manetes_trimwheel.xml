<!-- ####################################################################### -->
<!-- Animations -->
<!-- ####################################################################### -->

    <animation>
    <type>rotate</type>
      <object-name>trim_disk</object-name>
      <property>controls/flight/elevator-trim</property>
      <factor>18000</factor>
      <axis>
        <x>0.0</x>
        <y>1.0</y>
        <z>0.0</z>
      </axis>
      <center>
        <x-m>-18.6368</x-m>
        <y-m>0.0</y-m>
        <z-m>0.487173</z-m>
      </center>
    </animation>  

    <!-- Throttle -->

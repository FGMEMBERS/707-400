<!-- Lampadas -->

<animation>
        <type>material</type>
        <object-name>metalring_L</object-name>
        <object-name>offlamp_L</object-name>
 <emission>
	 <red>0.5</red>
	 <green>0.45</green>
	 <blue>0.42</blue>
	 <factor-prop>controls/lighting/panel-norm</factor-prop>	
 </emission>
</animation>

<animation>
        <type>select</type>
      <object-name>onlamp_L</object-name>
        <condition>
	      <greater-than>
	        <property>controls/gear/brake-parking</property>
	        <value>0</value>
	      </greater-than>
        </condition>
</animation>  

<animation>
        <type>select</type>
      <object-name>offlamp_L</object-name>
        <condition>
	      <equals>
	        <property>controls/gear/brake-parking</property>
	        <value>0</value>
	      </equals>
        </condition>
</animation>  


<animation>
        <type>material</type>
        <object-name>metalring_R</object-name>
        <object-name>offlamp_R</object-name>
 <emission>
	 <red>0.5</red>
	 <green>0.45</green>
	 <blue>0.42</blue>
	 <factor-prop>controls/lighting/panel-norm</factor-prop>	
 </emission>

	      <equals>
	        <property>controls/flight/flaps</property>
	        <value>0</value>
	      </equals>
        </condition>
</animation>  

<animation>
        <type>select</type>
      <object-name>onlamp_R</object-name>
        <condition>
	      <greater-than>
	        <property>controls/flight/flaps</property>
	        <value>0</value>
	      </greater-than>
        </condition>
</animation>  
		
		



<!-- Iluminacao -->

	
<animation>
      <type>material</type>
      <object-name>trim_disk</object-name>
      <object-name>manete_full</object-name>
      <emission>
            <red>0.3</red>
            <green>0.25</green>
            <blue>0.22</blue>
            <factor-prop>controls/lighting/panel-norm</factor-prop>
      </emission>
</animation>

<!-- *** LoD *** -->
<animation>
      <type>range</type>

<?xml version="1.0" ?> 
  <PropertyList> 
  
    <path>manetes_trim.ac</path> 
   
<!-- ####################################################################### -->
<!-- Object set -->
<!-- ####################################################################### -->
   
    <animation>
    <name>manete_full</name>
      <object-name>Throttle0</object-name>
      <object-name>Throttle1</object-name>
      <object-name>Throttle2</object-name>
      <object-name>Throttle3</object-name>
      <object-name>Rev_Throttle0</object-name>
      <object-name>Rev_Throttle1</object-name>
      <object-name>Rev_Throttle2</object-name>
      <object-name>Rev_Throttle3</object-name>
    </animation>  

    <animation>
    <name>manete_comp_0</name>
      <object-name>Rev_Throttle0</object-name>
      <object-name>Throttle0</object-name>
    </animation>  

    <animation>
    <name>manete_comp_1</name>
